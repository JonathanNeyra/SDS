<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SdsData" table="sds_data">
    <id name="idSds" type="integer" column="id_sds">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titleSds" type="string" column="title_sds" length="200" nullable="false"/>
    <field name="manufacSds" type="string" column="manufac_sds" length="150" nullable="false"/>
    <field name="itemSds" type="string" column="item_sds" length="5" nullable="false"/>
    <field name="dirFileSds" type="string" column="dir_file_sds" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
